{
    "name": "Alone-music bot",
    "description": "MusicUserBot To Play Songs And Videos In Telegram Voice Chat.",
    "logo": "https://telegra.ph/file/e5646ddccc68d2942b697.jpg",
    "keywords": [
        "Telegram",
        "MusicPlayer",
        "VideoPlayer"
    ],
    "repository": "https://github.com/i-m-pro-king/Alone.s-music",
    "env": {
        "SESSION": {
            "description": "Pyrogram String Session",
            "required": true
        },
        "API_ID": {
            "description": "Enter APP ID Please Download from my.telegram.org/apps",
            "required": true
        },
        "HNDLR": {
            "description": "it will be used as prefix in all commands",
            "required": true,
            "value": "+"
        },
        "API_HASH": {
            "description": "Input HASH API Please Download from my.telegram.org/apps",
            "required": true
        },
        "SUDO_USERS": {
            "description": "List of User ID users count as admin everywhere (separate using space)",
            "required": true
        }
    },
    "addons": [],
    "buildpacks": [
        {
            "url": "heroku/python"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    },
    "stack": "container"
}
